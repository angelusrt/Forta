{
  "expo": {
    "name": "Forta",
    "slug": "Forta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/forta-icon.png",
    "splash": {
      "image": "./assets/forta-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003573"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/forta-adaptive-icon.png",
        "backgroundColor": "#003573"
      },
      "package": "com.bullgado.Forta"
    },
    "web": {
      "favicon": "./assets/forta-favicon.png"
    }
  }
}
